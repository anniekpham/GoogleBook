  },
  "homepage": "https://github.com/anniekpham/GoogleBook#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.0",
    "reactstrap": "^8.0.0"
  }
}
